/* -----------------------------------*/
/* ---------->>> GLOBAL <<<-----------*/
/* -----------------------------------*/

html {
	font-size: 62.5%
}

html.turbolinks-progress-bar::before {
    background-color: $bezier !important;
    height: 2px !important;
}

body {
	text-rendering: geometricPrecision;
    font-family: $proxima-nova;
    font-weight: 300;
    font-size: 1.6rem;
    text-shadow: 0 0 1px transparent;
    line-height: 1.5em;
    color: $body;
}

h1, h2, h3, h4, h5, h6 {
    font-weight: 300;
    color: $heading;
}

h1 {
    font-size: 4.4rem;
}

h2 {
    font-size: 3.2rem;
}

h3 {
	font-size: 2.8rem;
}

strong {
	font-weight: 300;
}


a {
    text-decoration: none !important;
    color: $heading;
}

a:hover {
    cursor: pointer;
    background: none;
    color: $heading;
}


/* FLASH MESSAGES */
.alert {
    border-radius: 0;
    background: $electric-blue;
    color: #ffffff;
    height: 40px;
    overflow: hidden;
    position: relative;
    top: 70px;
    width: 100%;
    z-index: 99999999999;
}

.alert ul li {
    margin-top: -10px;
    list-style: none;
    text-align: center;
    font-size: 14px;
    text-transform: uppercase;
    font-family: $proxima-nova;
}

/* FLASH MESSAGES IN FORMS */
span.help-inline {
    margin-left: 10px;
    color: red;
}




