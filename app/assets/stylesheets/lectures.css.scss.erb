//--------------------------------------------------------------
// NAVBAR
// --------------------------------------------------------------


/* navbar */

nav {
    height: 50px;
}

.navbar-nav > li > a {
    font-family: $proxima-nova;
    font-weight: 100;
    padding-top: 20px !important;
    color: #fff !important;
    font-size: 16px;
    text-decoration: none;
}

nav.navbar-home {
    background: none;
	/*margin-top: -70px;*/
    padding-top: 20px;
    text-transform: uppercase;
}

nav.navbar-home a {
    font-size: 13px !important;
    font-weight: 500 !important;
    padding: 5px 25px;
}

nav.navbar-home.shrink-home a:hover {
    color: $bezier !important;
}

nav.navbar-home.shrink-home, nav.navbar-home.final {
    background: white !important;
    margin-top: 0px;
    padding-top: 0px;
    border-bottom: 1px solid $mercury;
    height: 60px;
}

.shrink-home, .final {
    background: white !important;
	/*margin-top: -60px;*/;
}

nav.navbar-home.navbar-initial {
	/*margin-top: -10px;*/;
}

.navbar-fixed-top {
    transition: all 0.5s ease;
}


/* navbar left */

.navbar-left li a {
    padding-top: 10px !important;
    font-size: 18px !important;
    font-weight: 700 !important;
}

/* navbar left hover */

.navbar-left li a:hover {
    color: $bezier !important;
}

/* navbar left shrinked */

nav.shrink-home .navbar-left li a,  nav.final .navbar-left li a {
    color: $bezier !important;
    padding-top: 20px !important;
    font-weight: 700 !important;
    font-size: 24px !important;
    transition: font-size 0.1s ease;
}

/* navbar left shrinked hover */

nav.shrink-home .navbar-left li a:hover, nav.final .navbar-left li a:hover {
    opacity: 0.9 !important;
}

/* navbar right */

.navbar-right li {
    padding: 0 10px;
}

p.navbar-btn {
    padding-top: 5px;
}

/* navbar right hover */

.navbar-right li a:hover {
    color: $bezier !important;
    transition: all 0.3s ease-in;
}

p.navbar-btn a:hover {
    color: #ffffff !important;
    transition: background 0.2s ease-in;
}

/* navbar right shrinked  */

nav.navbar-home.shrink-home a, nav.navbar-home.final a {
    color: #8A959E !important;
}

nav.navbar-home.shrink-home a.btn, nav.navbar-home.final a.btn {
    color: #ffffff !important;
    background: $bezier;
    border: 1px solid $bezier !important;
}

nav.navbar-home.final a {
    color: #8A959E !important;
}

nav.navbar-home.final a.btn {
    color: #ffffff !important;
    background: $bezier;
    border: 1px solid $bezier !important;
}

/* navbar right shrinked hover */

nav.navbar-home.shrink-home a.btn:hover {
    opacity: 0.8;
}

nav.navbar-home.final a.btn:hover {
    opacity: 0.8;
}






