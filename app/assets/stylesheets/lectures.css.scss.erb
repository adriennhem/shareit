/* PROGRESS BAR */

progress[value] {
   -webkit-appearance: none;
   appearance: none;
	height:15px;
	width:100%;
}

progress[value]::-webkit-progress-bar {
  background-color:#E2E5E9;
  border-radius: 10px;
}

progress[value]::-webkit-progress-value {
  background:#e67e22;
  background-image:
	   -webkit-linear-gradient(-45deg, 
	                           transparent 40%,  
	                            transparent 66%),
	   -webkit-linear-gradient(left, $bezier 33%, $bezier, $bezier, $bezier);

  border-radius: 10px; 
  background-size: 45px 25px, 100% 100%, 100% 100%;
}

 
