@import 'config';
@import "bootstrap-sprockets";
@import "bootstrap";

// --------------------------------------------------------------
// GENERAL SETTINGS
// --------------------------------------------------------------

html {text-rendering:optimizeLegibility;}

html.turbolinks-progress-bar::before {
  background-color: $bezier !important;
  height: 2px !important;
}

html a {text-decoration:none !important;}

a:hover {cursor:pointer; background: none;}

/* CREATE WHITE BG WHEN NOTIFICATION */
.container-application {margin-top: -20px;}


/* WHY DO WE HAVE THIS HALANT? */
body {
	font-family:$proxima-nova;
	font-weight:400;
	/*padding-top:60px;*/
	font-size:16px;
	color:$steel;
	line-height:32px;
}

h1, h2, h3, h4, h5, h6 {
	color:$black-pearl;
 	font-family:$proxima-nova;
 	font-weight:800;
 	}

h1 {font-size:32px; color:$mine-shaft;}

h2 {font-size:28px;}


/* FLASH MESSAGES */
.alert {
	border-radius:0;
	background:$electric-blue;
	 color:#ffffff;
	 height:40px;
	}

.alert ul li {
	margin-top:-10px;
	list-style: none; 
	text-align: center; 
	font-size: 14px; 
	text-transform: uppercase;
	font-family:$proxima-nova;
}

/* FLASH MESSAGES IN FORMS */
span.help-inline {margin-left: 10px; color:red;}


 // ----- COLORS ---- //

.shamrock {color:$shamrock;}

.white {color:white;}

  // ----- BACKGROUNDS ---- //

.bg-grey {background: #F9F9FB;}

.bg-concrete {background:#F4F4F4;}

.bg-footer {background:#ffffff;}

.bg-white {background: $white;}


// --------------------------------------------------------------
// BORDERS
// --------------------------------------------------------------

.picture {border-radius:150px;}

.border-top {border-top:1px solid #ebebeb;}

// --------------------------------------------------------------
// SIZES
// --------------------------------------------------------------

 // ----- WIDTH ---- //


.width300 {width:300px;}

.width550 {width:550px; margin: 0 auto;}

.width700 {width:700px; margin: 0 auto;}

.width960 {max-width:960px; margin:0 auto;}

 // ----- HEIGHT ---- //

.height610 {height:610px}

// --------------------------------------------------------------
// POSITION
// --------------------------------------------------------------

.vertical {position:relative; top:50%; transform: translateY(-50%);}

 // ----- MARGIN ---- //

.margin-s {margin-top:40px;}

.margin-top-xs {margin-top:20px;}

.margin-s-l {margin-left:10px;}

.margin-l-l {margin-left:50px;}

.mb-60 {margin-bottom:60px;}

.mb-150 {margin-bottom:150px}

.mt-negative-15 {margin-top:-15px; }

.margin-top-negative {margin-top:-70px;}

 // ----- PADDINGS ---- //

.pad10 {padding:10px 10px}

.pad5 {padding:5px 5px}

// ----- FLOAT ----- // 

.center {text-align:center;}

.float-left {float:left;}

.float-right {float:right;}

.clear {clear:both;}





// --------------------------------------------------------------
// BUTTONS
// --------------------------------------------------------------

input.btn-bezier, a.btn-bezier, button.btn-bezier {
	color:#fff !important;
	background:$bezier !important;
	border-color:$bezier !important;
	font-size:16px;
	border-radius:0 !important;
	text-shadow:none !important;
	box-shadow:none !important;

}

a.btn-bezier-inverse {
	color:$bezier !important;
	border-color:$bezier !important;
	font-size:16px;
	border-radius:0;
}

a.btn-bezier-inverse:hover {
	color:#ffffff !important;
	background:$bezier !important;
	border-color:$bezier !important;
	font-size:16px;
	border-radius:0;
	transition:all 0.2s ease-in;
}

button.btn-bezier {
	font-size:14px;
	font-weight:300;
	text-shadow: none;
	color:$bezier;
	background:none;
	border:none;
	box-shadow: none;
}

button.btn-bezier:hover {
	color:$bezier;
	background:none;
	border:none;
	box-shadow:none;
}



a.btn, button.btn, input.btn {font-weight: 700; font-family: $proxima-nova; text-transform: uppercase; text-shadow:none; box-shadow: none;}

a.btn-orange {
	color:#fff !important;
	background:$orange-peel !important;
	border-color:$orange-peel !important;
	font-size:18px;
	border-radius:0;
}

a.btn-orange:hover {
	background:$orange-peel !important;
	border-color:$orange-peel !important;
	transition:background 0.2s ease-in;
}


button.btn-orange, input.btn-orange {
	color:#fff !important;
	background:$orange-peel !important;
	border-color:$orange-peel !important;
	font-size:18px;
	border-radius:0 !important;
}

button.btn-orange:hover, input.btn-orange:hover {
	background:$orange-peel !important;
	border-color:$orange-peel !important;
	transition:background 0.2s ease-in;
}

a.btn-shaft {
	color:#fff !important;
	background:$mine-shaft !important;
	border-color:$mine-shaft !important;
	font-size:16px;
	border-radius:0;
	padding:16px 30px;
}


button.btn-form, input.btn-form {
	padding:15px 50px;
	text-decoration:none;
	color:#fff !important;
	background:$mine-shaft !important;
	border-color:$mine-shaft !important;
	font-size:14px;
	border-radius:0 !important;
	/*text-transform:uppercase;*/
}

button.btn-form:hover, input.btn-form:hover {
	transition:all 0.1s ease-in;
	background:$shamrock !important;
	border-color:$shamrock !important;
}

a.lesson.btn-orange {
	padding: 15px 20px;
	font-size: 14px;
}


// --------------------------------------------------------------
// NAVBAR
// --------------------------------------------------------------


/* navbar */

nav {height:50px;}

.navbar-nav > li > a {
  font-family: $proxima-nova;
  font-weight:100;
  padding-top:20px !important;
  color:#fff !important;
  font-size: 16px;
  text-decoration: none;
}

nav.navbar-home {
	background: none;
	/*margin-top: -70px;*/
	padding-top: 20px;
	text-transform: uppercase;

}

nav.navbar-home a {
	font-size: 13px !important;
	font-weight:500 !important;
	padding:5px 25px;
}

nav.navbar-home.shrink-home a:hover {
	color:$bezier !important;
}


nav.navbar-home.shrink-home {
	background: white !important;
	margin-top:0px;
	padding-top: 0px;
	border-bottom:1px solid $mercury;
	height:60px;
}

.shrink-home {
	background: white !important;
	/*margin-top: -60px;*/
}

nav.navbar-home.navbar-initial {
	/*margin-top: -10px;*/

}

.navbar-fixed-top {
	transition: all 0.5s ease;
}


/* navbar left */

.navbar-left li a {
	padding-top:10px !important;
	font-size:18px !important;
	font-weight:700 !important;
}

/* navbar left hover */

.navbar-left li a:hover {
	color:$bezier !important;
}

/* navbar left shrinked */

nav.shrink-home .navbar-left li a {
	color:$bezier !important;
	padding-top:20px !important;
	font-weight:700 !important;
	font-size:24px !important;
	transition:font-size 0.1s ease;
}

/* navbar left shrinked hover */

nav.shrink-home .navbar-left li a:hover {
	opacity:0.9 !important;
}

/* navbar right */

.navbar-right li {
	padding:0 10px;
}

p.navbar-btn {
	padding-top:5px;
}

/* navbar right hover */

.navbar-right li a:hover {
	color:$bezier !important;
	 transition: all 0.3s ease-in;
}

p.navbar-btn a:hover {
	color:#ffffff !important;
	transition:background 0.2s ease-in;
}

/* navbar right shrinked  */

nav.navbar-home.shrink-home a {
	color: #8A959E !important;
}

nav.navbar-home.shrink-home a.btn {
	color:#ffffff !important;
	background:$bezier;
	border:1px solid $bezier !important;
}

/* navbar right shrinked hover */

nav.navbar-home.shrink-home a.btn:hover {
	opacity:0.8;
}



