<p id="notice"><%= notice %></p>


<h1 class="page-header">Web Development</h1>



    <% @lectures.each do |lecture| %>
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">
          <%= link_to lecture.title, lecture %></h3>
      </div>
      <div class="panel-body">
      <%= image_tag lecture.picture.url(:thumb) %>
      <%= lecture.user.email %> <br>
      <%= lecture.category.name %> <br> 
      <%= lecture.description %>
      </div>
      <div class="panel-footer">
         <% if current_user == lecture.user %>
         <%= link_to 'Edit', edit_lecture_path(lecture) %>
         <%= link_to 'Destroy', lecture, method: :delete, data: { confirm: 'Are you sure?' } %>
          <% end %>
      </div>
    </div>
       <% end %>

<br>

