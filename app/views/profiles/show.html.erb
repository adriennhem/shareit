 <script type="text/javascript">
    mixpanel.track("Visit Dashboard Home")
</script>

<% if current_user.student? %>
	 <%= render 'profiles/student' %>
<% elsif current_user.admin? %>
	<%= link_to 'Go to admin dashboard', admin_dashboard_path, class: 'btn btn-large btn-bezier' %>
<% elsif current_user.institution? %>
	<%= render 'profiles/institution' %>
<% else %>	
	<%= render 'profiles/company' %>
<% end %>


