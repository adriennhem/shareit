<div class="container" id="signup">  
    <h1>Sign In</h1>
    <div id="signup-form">
    <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>
      
       <%= f.input :email %>
       <%= f.input :password %>
       <%= f.button :submit, 'Sign In', class: "btn-orange btn" %>
    <% end %>
    
    <%= render "devise/shared/links" %>

    </div>  <!-- end .panel-lock -->
</div> <!-- end #content -->

